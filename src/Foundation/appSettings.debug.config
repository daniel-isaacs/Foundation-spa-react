  <!-- Profile Store API configuration -->
  <add key="ProfileStore.Url" value="https://profilesapi-eastus01.profilestore.episerver.net/" />
  <add key="ProfileStore.SubscriptionKey" value="V25hYIDigi0Sn1YVOHyPyYEgAVRBbfaBUASjao5zvZHPlQcs" />
