{
  "name": "foundation-spa-frontend",
  "description": "Frontend for Episerver Foundation CMS built using TypeScript & React",
  "author": "Remko Jantzen",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "sideEffects": true,
  "scripts": {
    "login": "npx epi-auth -e development",
    "sync-models": "npx epi-sync-models -e development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "echo \"Clean: not implemented\" && exit 0",
    "start": "npx webpack serve --env EPI_ENV=development --env NODE_ENV=development --config webpack.dev.config.js",
    "build": "npx webpack --env EPI_ENV=development --env NODE_ENV=development",
    "build:server": "npx webpack --env EPI_ENV=development --env NODE_ENV=development  --config webpack.server.config.js",
    "build:website": "npx webpack --env EPI_ENV=development --env NODE_ENV=development  --config webpack.frontend.config.js",
    "build:production": "npx epi-sync-models -e production && npx webpack --env EPI_ENV=production --env NODE_ENV=production && npx epi-sync-models -e development",
    "watch": "npx webpack --watch --progress --env EPI_ENV=development",
    "watch-css": "npx tsm src/**/*.module.scss --aliasPrefixes.~ ./node_modules/ --implementation sass --watch",
    "compile": "npx tsc --pretty --project ./tsconfig.json"
  },
  "dependencies": {
    "@episerver/foundation-settings": "file:lib/foundation-settings",
    "@episerver/spa-core": "github:episerver/foundation-lib-spa-core#develop",
    "@fullcalendar/daygrid": "^5.4.0",
    "@fullcalendar/list": "^5.4.0",
    "@fullcalendar/react": "^5.4.0",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.0",
    "bootstrap": "^5.0.2",
    "deep-equal": "^2.0.5",
    "eventemitter3": "^4.0.7",
    "html-entities": "^2.0.6",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.19",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0",
    "react-leaflet": "^3.1.0",
    "react-player": "^2.8.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@episerver/webpack": "github:episerver/foundation-lib-spa-webpack#develop",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/bootstrap": "^5.0.16",
    "@types/deep-equal": "^1.0.1",
    "@types/json-schema": "^7.0.7",
    "@types/leaflet": "^1.5.23",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.0.1",
    "dotenv-expand": "^5.1.0",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.1",
    "html-inline-css-webpack-plugin": "^1.11.0",
    "html-webpack-plugin": "^5.1.0",
    "json-schema": "^0.3.0",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^2.0.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.33.0",
    "sass-loader": "^12.0.0",
    "schema-utils": "^3.0.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.1",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-merge": "^5.7.3",
    "yargs": "^17.0.1"
  }
}
