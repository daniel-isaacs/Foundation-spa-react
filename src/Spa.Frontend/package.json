{
  "name": "foundation-spa-frontend",
  "version": "1.0.0",
  "description": "Frontend for Episerver Foundation CMS built using TypeScript & React",
  "private": true,
  "scripts": {
    "sync-models": "npx epi-sync-models",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "echo \"Clean: not implemented\" && exit 0",
    "build": "npx webpack --env.NODE_ENV=development",
    "build-html": "npx webpack --env.NODE_ENV=development --env.EPI_BUILD_TARGET=html",
    "build-express": "npx webpack --env.NODE_ENV=development --env.EPI_BUILD_TARGET=express",
    "build-episerver": "npx webpack --env.NODE_ENV=development --env.EPI_BUILD_TARGET=episerver",
    "release": "npx epi-sync-models && npx webpack --env.NODE_ENV=production",
    "watch-episerver": "npx epi-sync-models && webpack --watch --progress --env.NODE_ENV=development --env.EPI_BUILD_TARGET=episerver",
    "run-html": "npx webpack-dev-server --env=NODE_ENV=development --config=html.webpack.config.js --open",
    "run-express": "npx webpack --env.NODE_ENV=development --env.EPI_BUILD_TARGET=express && node ./dist/server.main.js",
    "compile": "npx epi-sync-models && tsc -p ./tsconfig.json"
  },
  "author": "Remko Jantzen",
  "license": "Apache-2.0",
  "dependencies": {
    "@episerver/spa-core": "git+https://github.com/episerver/foundation-lib-spa-core.git#Develop",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "html-entities": "^1.3.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0"
  },
  "devDependencies": {
    "@episerver/webpack": "git+https://github.com/episerver/foundation-lib-spa-webpack.git#Develop",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@types/bootstrap": "^4.3.3",
    "@types/core-js": "^2.5.4",
    "@types/express": "^4.17.8",
    "@types/html-entities": "^1.2.16",
    "@types/jquery": "^3.5.1",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/reactstrap": "^8.5.1",
    "@types/webpack-dev-middleware": "^3.7.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.2",
    "precss": "^4.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.2",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4",
    "webpack-node-externals": "^2.5.2"
  }
}
